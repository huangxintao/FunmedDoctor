<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:sky="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:fitsSystemWindows="true"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <com.sky.health.view.HeaderView
        android:layout_alignParentTop="true"
        sky:headerViewMidText="支付结果"
        android:id="@+id/headerView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        sky:headerViewLeftIcon="@mipmap/back_arrow_icon"
        android:background="@color/dot_color"/>
    <TextView
        android:textSize="18sp"
        android:layout_centerInParent="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/payResultTextView"/>
</RelativeLayout>